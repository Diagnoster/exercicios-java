package arrays;

import java.util.Arrays;

public class Exercicio {

	public static void main(String[] args) {
		
		double[] notasAlunoA = new double[3]; // tipo de array, declaração array [], nome e quantidade elementos
		notasAlunoA[0] = 7.9;
		notasAlunoA[1] = 8;
		notasAlunoA[2] = 6.7;
		
		Arrays
		System.out.println(notasAlunoA);
		
	}
}
